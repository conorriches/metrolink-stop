{
  "name": "metrolink-stop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "regenerate-gtfs-lines": "npm run gtfs-prepare && npm run gtfs-lines",
    "regenerate-gtfs-isochrones": "npm run gtfs-prepare && npm run gtfs-isochrones",
    "gtfs-prepare": "(cd ./data/lines; bash prepare.sh)",
    "gtfs-lines": "(cd ./data/lines;  NODE_OPTIONS='--experimental-json-modules' node process-lines.mjs)",
    "gtfs-isochrones": "(cd ./data/lines; Rscript calculate-stop-times.R && NODE_OPTIONS='--experimental-json-modules' node process-isochrones.mjs)",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --pretty --noEmit"
  },
  "pre-commit": [
    "lint",
    "type-check"
  ],
  "dependencies": {
    "@rjackson/rjds": "^0.4.0",
    "fathom-client": "^3.4.1",
    "fuse.js": "^6.4.6",
    "next": "^12.1.6",
    "odata": "^1.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "react-spinners": "^0.12.0",
    "react-use": "^17.4.0",
    "styled-components": "^5.3.5",
    "swr": "^1.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.13",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.3.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "gtfs": "^3.2.4",
    "postcss": "^8.4.19",
    "pre-commit": "^1.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  }
}
